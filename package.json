{
  "name": "boke-devops",
  "version": "0.0.30",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "sa": "node ./scripts/index.mjs",
    "dev": "cd packages/boke-docs && npm run dev",
    "build": "cd packages/boke-devops-vue && npm run build",
    "release": "pnpm run sa release",
    "publish": "cd packages/boke-devops-vue && npm publish",
    "test": "vitest --ui",
    "test:snap": "vitest --ui -u"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.4",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "1.6.0",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^9.4.1",
    "c12": "^1.11.1",
    "cac": "^6.7.14",
    "jsdom": "^24.1.0",
    "kolorist": "^1.8.0",
    "lerna": "^8.1.5",
    "less": "^4.2.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0",
    "vitest-fetch-mock": "^0.2.2",
    "vue-router": "^4.4.0"
  },
  "dependencies": {
    "@dumijs/preset-vue": "^2.3.0",
    "unplugin-vue-macros": "^2.9.5",
    "vue": "^3.4.31"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umijs/dumi",
    "directory": "@devops/boke-docs"
  }
}
